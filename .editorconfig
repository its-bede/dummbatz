# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

[*.{js,rb,js.erb,html.erb,yml,erb}]
# Set default charset
charset = utf-8

# basic conventions
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
max_line_length = 128